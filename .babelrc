{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime",
    [
      "module-resolver",
      {
        "alias": {
          "@components": "./src/components",
          "@pages": "./src/pages",
          "@constants": "./src/constants",
          "@services": "./src/services",
          "@hooks": "./src/hooks",
          "@helpers": "./src/helpers",
          "@redux": "./src/redux"
        }
      }
    ]
  ]
}
